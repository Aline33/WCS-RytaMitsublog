{{ dump(previousId) }}


<div class="container-fluid d-flex justify-content-between align-items-center"  style="background-color: #0A9A93; color: #FDFFF5;"  id="bt-Article-Prev-Next-bar">
    {% if previousId is not null %}
        <div class="col-3 d-flex" id="bt-Article-Previous-Article">
            <a href="article/show', {'id': previousArticle['id']}">
                <img src="/public/assets/images/logoProjet2.png" alt="" style="max-width: 200px">
                <h5>{{ previousId['title'] }}</h5>
            </a>
        </div>
    {% endif %}
    {% if nextId is not null %}
        <div class="col-3 d-flex" id="bt-Article-Next-Article">
            <a href="article/show', {'id': nextArticle['id']}">
                <h5>{{ nextId['title'] }}</h5>
                <img src="/public/assets/images/logoProjet2.png" alt="" style="max-width: 200px">
            </a>
        </div>
    {% endif %}
</div>


